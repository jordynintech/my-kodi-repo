<?xml version='1.0' encoding='utf-8'?>
<addons>
  <addon id="repository.jordynintech" name="Jordyn's Kodi Repository" version="1.0.1" provider-name="Jordyn in Tech">
    <extension point="xbmc.addon.repository" name="Jordyn's Kodi Repository">
      <dir>
        <info compressed="false">https://raw.githubusercontent.com/jordynintech/my-kodi-repo/main/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/jordynintech/my-kodi-repo/main/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/jordynintech/my-kodi-repo/main/zips/</datadir>
      </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary>A personal repository of my favorite Kodi addons.</summary>
      <description>This repository contains my favorite add-ons and is not official.</description>
      <platform>all</platform>
    </extension>
  </addon>
  <addon id="plugin.video.umbrella" name="Umbrella" version="6.7.47" provider-name="Umbrella">
    <requires>
      <import addon="xbmc.python" version="3.0.0" />
      <import addon="script.module.requests" />
      <import addon="script.module.routing" />
      <import addon="script.module.unidecode" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
      <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary lang="en_GB">Umbrella</summary>
      <description lang="en_GB">Umbrella addon for movies and TV shows</description>
      <platform>all</platform>
      <license>GPL-2.0-or-later</license>
      <website>https://github.com/umbrellaplug</website>
      <assets>
        <icon>resources/media/icon.png</icon>
        <fanart>resources/media/fanart.jpg</fanart>
      </assets>
    </extension>
  </addon>
  <addon id="script.module.cocoscrapers" name="CocoScrapers Module" version="1.0.1" provider-name="CocoScrapers">
    <requires>
      <import addon="xbmc.python" version="3.0.0" />
    </requires>
    <extension point="xbmc.python.module" library="lib" />
    <extension point="xbmc.addon.metadata">
      <summary lang="en_GB">CocoScrapers Module</summary>
      <description lang="en_GB">External scraper module for Kodi video add-ons</description>
      <platform>all</platform>
      <license>GPL-3.0-only</license>
      <assets>
        <icon>icon.png</icon>
      </assets>
    </extension>
  </addon>
</addons>